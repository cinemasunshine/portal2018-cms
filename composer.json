{
    "name": "cinemasunshine/portal2018-admin",
    "description": "Cinemasunshine Portal2018 Admin",
    "type": "project",
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Cinemasunshine\\PortalAdmin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "~7.2",
        "slim/slim" : "^3.12",
        "slim/twig-view": "^2.5",
        "slim/flash": "^0.4.0",
        "monolog/monolog": "^1.23",
        "vlucas/phpdotenv": "^2.5",
        "doctrine/orm": "2.6.*",
        "laminas/laminas-validator": "^2.13",
        "laminas/laminas-form": "^2.14",
        "laminas/laminas-i18n": "^2.10",
        "laminas/laminas-i18n-resources": "^2.6",
        "laminas/laminas-session": "^2.9",
        "laminas/laminas-uri": "^2.7",
        "microsoft/azure-storage-blob": "^1.5",
        "intervention/image": "^2.4",
        "symfony/console": "^4.3",
        "symfony/filesystem": "^4.3"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.5",
        "phpstan/phpstan": "^0.12",
        "phpstan/phpstan-doctrine": "^0.12",
        "phpunit/phpunit": "^8.5",
        "mockery/mockery": "^1.3",
        "phpstan/phpstan-phpunit": "^0.12.8",
        "phpstan/phpstan-mockery": "^0.12.4"
    },
    "scripts": {
        "phpcs": "vendor/bin/phpcs --standard=phpcs.xml",
        "phpcbf": "vendor/bin/phpcbf --standard=phpcs.xml",
        "phpstan": "vendor/bin/phpstan analyse -n --no-progress -c phpstan.neon",
        "phpunit": "vendor/bin/phpunit"
    }
}
