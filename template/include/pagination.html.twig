<nav aria-label="Page navigation">
    <ul class="pagination justify-content-center">
        
        <li class="page-item {% if pagenater.isFirstPage() %}disabled{% endif %}">
            {% set params = params|merge({ 'p': pagenater.getPreviousPage() }) %}
            <a class="page-link" href="{{ path_for('title_list', {}, params) }}">&laquo;</a>
        </li>
        
        {% for link in pagenater.getLinks() %}
            <li class="page-item {% if pagenater.getPage() == link %}active{% endif %}">
                {% set params = params|merge({ 'p': link }) %}
                <a class="page-link" href="{{ path_for('title_list', {}, params) }}">{{ link }}</a>
            </li>
        {% endfor %}
        
        <li class="page-item {% if pagenater.isLastPage() %}disabled{% endif %}">
            {% set params = params|merge({ 'p': pagenater.getNextPage() }) %}
            <a class="page-link" href="{{ path_for('title_list', {}, params) }}">&raquo;</a>
        </li>
        
    </ul>
</nav>